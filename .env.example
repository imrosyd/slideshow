ADMIN_PASSWORD=

# Database Configuration
# Default: SQLite (file:./prisma/dev.db) - No configuration needed!
# Optional: Use PostgreSQL for production
# DATABASE_URL=postgresql://user:password@host:port/database?schema=public

# Supabase Configuration (OPTIONAL - Only if you want to use Supabase)
# If not set, app will automatically use Prisma with SQLite/PostgreSQL
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
# SUPABASE_SERVICE_ROLE_KEY=

# Storage Configuration (Optional)
# Default: Filesystem storage in ./storage folder
# Set USE_FILESYSTEM_STORAGE=false to use Supabase Storage
# USE_FILESYSTEM_STORAGE=true
# STORAGE_PATH=./storage
# STORAGE_PUBLIC_URL=/api/storage

# Supabase Storage (Only if using Supabase Storage)
# SUPABASE_STORAGE_BUCKET=
# SUPABASE_DURATIONS_TABLE=
