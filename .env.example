ADMIN_PASSWORD=

# Database Configuration (Priority: DATABASE_URL > Supabase)
# Option 1: Use Prisma with PostgreSQL (Recommended)
# Set DATABASE_URL to connect to any PostgreSQL database (including Supabase's PostgreSQL)
# DATABASE_URL=postgresql://user:password@host:port/database?schema=public

# Option 2: Use Supabase SDK (Fallback)
# If DATABASE_URL is not set, the app will automatically use Supabase SDK
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Supabase Storage (Always used for file storage, regardless of database choice)
SUPABASE_STORAGE_BUCKET=
SUPABASE_DURATIONS_TABLE=
